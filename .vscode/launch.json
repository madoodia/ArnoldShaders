  "version": "0.2.0",
  "configurations": [
    {
      "type": "cppdbg",
      "name": "Debugger",
      "request": "launch",
      "program": "${workspaceFolder}/${relativeFileDirname}/${fileBasenameNoExtension}.cpp",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build_debug",
      "environment": [],
      "externalConsole": false,
      "windows": {
        "miDebuggerPath": "C:/Program Files (x86)/Microsoft Visual Studio/2019/Community/VC/Tools/MSVC/14.27.29110/bin/Hostx64/x64/cl.exe"
      },
      "linux": {
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "miDebuggerPath": "/opt/rh/devtoolset-9/root/bin/gdb"
      },
      "osx": {},
    }
  ]
}